int row = tblCourse.getSelectedRow()+1;
        if(row >= tblCourse.getRowCount()-1) {
            row = 0;
        }
        tblCourse.setRowSelectionInterval(row, row);
        setFormCourse(coursedao.selectByID(String.valueOf(tblCourse.getValueAt(row, 0))));
        tabsCourse.setSelectedIndex(1);